{
  "name": "template",
  "version": "1.0.0",
  "description": "Project template",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass scss/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass scss/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/style.comp.css",
    "prefix:css": "postcss css/style.concat.css --use autoprefixer -d css",
    "compress:css": "node-sass css/style.concat.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "author": "Marcin Jadwi≈ºyc",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.2.1",
    "concat": "^1.0.3",
    "npm-run-all": "^4.1.3",
    "postcss": "^7.0.5",
    "postcss-cli": "^6.0.1"
  },
  "browserslist": "last 10 versions"
}
